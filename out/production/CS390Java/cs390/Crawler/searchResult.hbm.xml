<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="cs390.Crawler.searchResult" table="searchResult">
    <meta attribute="class-description">
        This class contains the searchResult detail.
    </meta>
    <id name="searchResultID" type="int" column="searchResultID"/>
    <property name="word" column="word" type="string"/>
    <many-to-one name="sURL" column="URLID"
                 class="cs390.Crawler.searchURL" not-null="true"/>
</class>
</hibernate-mapping>